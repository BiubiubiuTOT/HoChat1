    <!--<item name="kswThumbHeight">@dimen/ksw_md_thumb_ripple_size</item>-->
    <!--</style>-->

    <style name="SwitchButtonStyle">
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
    </style>

    <!-- 右上角popupwindow -->
    <style name="AnimationPreview">
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
        <item name="android:windowExitAnimation">@anim/fade_out</item>
    </style>

    <style name="AnimationPage">
        <item name="android:windowEnterAnimation">@anim/slide_in</item>
        <item name="android:windowExitAnimation">@anim/slide_out</item>
    </style>


    <style name="ac_search_list_view_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:paddingLeft">14dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#999999</item>
        <item name="android:background">#ffffff</item>
    </style>

    <style name="ac_search_list_view_item_more">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#7ca1c9</item>
    </style>

    <style name="ac_search_list_view_divider">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#f0f0f6</item>
        <item name="android:layout_marginLeft">14dp</item>
        <item name="android:layout_marginRight">14dp</item>
    </style>

    <style name="ac_search_part_divider_high">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">14dp</item>
        <item name="android:background">#f0f0f6</item>
    </style>

    <style name="ac_search_no_results">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginTop">45dp</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#999999</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="ac_horizontal_light_thin_divider">
        <item name="android:layout_height">0.5dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">#E5E5E5</item>
    </style>

    <color name="group_list_black_textview">#353535</color>
    <color name="group_list_gray">#999999</color>
    <color name="de_title_bg">#0195ff</color>


    <!--输入框基本样式-->
    <style name="EditBaseStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_grey_666666</item>
        <item name="android:textColorHint">@color/color_grey_999999</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:lines">1</item>
        <item name="android:inputType">text</item>
    </style>

    <!--登录输入框样式-->
    <style name="EditLoginStyle" parent="EditBaseStyle">
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/bg_frame_login_edit</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:drawablePadding">10dp</item>
    </style>
    <!--主颜色圆角按钮-->
    <style name="ButtonCommonStyle" parent="@style/Widget.AppCompat.Button.Borderless">  
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/bg_rectangle_color_main_20dp</item>
        <item name="android:textColor">@color/color_white</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="AppTheme1" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="android:windowBackground">@android:color/white</item>
    </style>
</resources>
