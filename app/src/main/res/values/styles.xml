<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!--通用下划线-->
    <style name="comDivider">
        <item name="android:background">@color/color_line_grey</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    <!--底部Tab RadioButton样式-->
    <style name="BottomTabRb">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:textColor">@color/bottom_tab_text_color</item>
        <item name="android:textSize">10sp</item>
        <item name="android:drawablePadding">4dp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>


    <style name="WinDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <!-- -->
    <style name="ActionBarSettingImg">
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ActionBarSettingTextView">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/group_list_black_textview</item>
        <item name="android:textSize">16sp</item>
    </style>
    <!-- 下划线-->
    <style name="PublicLineView">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@color/group_list_gray</item>
    </style>

    <!--主页面ui下划线-->
    <style name="DemoMainPublic">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">4dp</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:background">@color/de_title_bg</item>
    </style>

    <style name="DemoMainNewMassage1">
        <item name="android:background">@drawable/rc_unread_count_bg</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/tab_indicator_text</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">6dp</item>
        <item name="android:layout_height">6dp</item>
        <!-- <item name="android:visibility">gone</item>-->
    </style>

    <style name="DemoMainNewMassage">
        <item name="android:background">@drawable/rc_unread_count_bg</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/tab_indicator_text</item>
        <item name="android:textSize">9sp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">18dp</item>
        <item name="android:layout_height">18dp</item>
        <!-- <item name="android:visibility">gone</item>-->
    </style>

    <style name="DemoAddressNewMassage">
        <item name="android:background">@drawable/rc_unread_count_bg</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/tab_indicator_text</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <!-- <item name="android:visibility">gone</item>-->
    </style>

    <!-- big black-->
    <style name="DemoTextStyleBig1">
        <item name="android:textSize">18sp</item>
    </style>
    <!-- big black-->
    <style name="DemoTextStyleBigBlack">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/group_list_black_textview</item>
    </style>
    <!-- small gray-->
    <style name="DemoTextStyleSmall">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/group_list_gray</item>
    </style>

    <!-- middle gray-->
    <style name="DemoTextStyleMiddle">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/group_list_gray</item>
    </style>


    <style name="anim_popup_dir">
        <item name="android:windowEnterAnimation">@anim/slide_in</item>
        <item name="android:windowExitAnimation">@anim/slide_out</item>
    </style>

    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">

        <item name="android:button">@drawable/de_pic_checkbox</item>

    </style>

    <style name="PicCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">

        <item name="android:button">@drawable/de_pic_chebox_photo</item>

        <item name="android:paddingLeft">2.0dip</item>

        <item name="android:maxHeight">10.0dip</item>

    </style>

    <style name="dialogFullscreen">
        <item name="android:windowFullscreen">true</item>
    </style>
    <!--SWITCH-->
    <!--<style name="SwitchButtonMD">-->
    <!--<item name="kswThumbDrawable">@drawable/ksw_md_thumb</item>-->
    <!--<item name="kswBackColor">@color/ksw_md_back_color</item>-->
